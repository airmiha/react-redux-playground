{    
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "env": {
        "mocha": true
    },
     "ecmaFeatures": {
        "jsx": true
    },
    "rules": {
        "indent": [2, 2],      
        "react/jsx-indent": [2, 2],
        "max-len": [2, 120]
    }
}